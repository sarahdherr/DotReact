{
  "name": "dotreact",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-jest && npm run test-mocha",
    "test-jest": "./node_modules/.bin/jest",
    "test-mocha": "mocha -r ts-node/register **/mocha-tests/**/*.spec.tsx",
    "build": "webpack",
    "build-watch": "webpack -w",
    "start": "node server/index.js"
  },
  "author": "Sarah Herr",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-infinite": "^0.12.1",
    "webpack": "^4.5.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.0.0-beta.43",
    "@types/chai": "^4.1.2",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.2",
    "@types/mocha": "^5.0.0",
    "awesome-typescript-loader": "^4.0.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "jsdom": "^11.7.0",
    "mocha": "^5.0.5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.1",
    "sinon": "^4.5.0",
    "source-map-loader": "^0.2.3",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1",
    "typings": "^2.1.1",
    "webpack-cli": "^2.0.13"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/jest-tests/**/*.spec.(ts|tsx|js)"
    ]
  }
}
